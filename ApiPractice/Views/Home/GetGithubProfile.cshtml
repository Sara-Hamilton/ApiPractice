@{
}

@model IEnumerable<ApiPractice.Models.GithubProfile>

<h2>All Messages</h2>
<hr />
<ol>
    @foreach (var profile in Model)
    {
        <li>
            <ul>
                <li>To: @Html.DisplayFor(modelItem => profile.Name)</li>
                <li>From: @Html.DisplayFor(modelItem => profile.Url)</li>
                <li>Body: @Html.DisplayFor(modelItem => profile.Location)</li>
                <li>Status: @Html.DisplayFor(modelItem => profile.Repos)</li>
            </ul>
        </li>
    }
</ol>